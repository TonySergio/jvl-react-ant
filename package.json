{
  "name": "jvl-react-ant",
  "version": "0.1.0",
  "description": "SAS Test Tool UI Application ",
  "private": true,
  "main": "src/main.js",
  "author": "antons",
  "scripts": {
    "postinstall": "install-app-deps",
    "precommit": "npm run lint-staged",
    "start": "cross-env ESLINT=none roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none roadhog dev",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*",
    "start:desktop": "electron . NODE_ENV=development",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "private:compile": "cross-env ESLINT=none roadhog build",
    "private:build:win": "build --win",
    "private:build:linux": "build --linux",
    "private:clean": "rimraf build"
  },
  "dependencies": {
    "@antv/data-set": "^0.9.0",
    "@babel/polyfill": "^7.0.0-beta.36",
    "antd": "^3.8.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.22.0",
    "bizcharts": "^3.1.10",
    "bizcharts-plugin-slider": "^2.0.1",
    "classnames": "^2.2.5",
    "dva": "^2.2.3",
    "dva-loading": "^2.0.3",
    "enquire-js": "^0.2.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.19.3",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^16.4.1",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.4.1",
    "react-fittext": "^1.0.0",
    "react-loadable": "^5.5.0",
    "rollbar": "^2.3.4",
    "setprototypeof": "^1.1.0",
    "url-polyfill": "^1.0.10"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.25.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "7.1.0",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "electron": "1.5.0",
    "electron-builder": "13.8.2",
    "electron-devtools-installer": "2.1.0",
    "electron-prebuilt-compile": "^2.0.7",
    "enzyme": "^3.4.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mockjs": "^1.0.1-beta3",
    "npm-run-all": "4.0.1",
    "prettier": "1.14.2",
    "pro-download": "^1.0.1",
    "promise": "7.1.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.6.2",
    "roadhog": "^2.4.2",
    "roadhog-api-doc": "^1.1.2",
    "stylelint": "^9.2.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.4.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
